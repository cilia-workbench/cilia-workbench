<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cilia-specifications>

	<!-- Sandbox -->

	<mediator-specification id="filter" namespace="fr.imag.adele.cilia.workbench.spec">
		<ports>
			<in-port name="in"/>
			<out-port name="out"/>
		</ports>
	</mediator-specification>

	<mediator-specification id="average-mediator" namespace="fr.imag.adele.cilia.workbench.spec">
		<ports>
			<in-port name="in"/>
			<out-port name="out"/>
		</ports>
		<properties>
			<property name="category"/>
		</properties>
	</mediator-specification>

	<mediator-specification id="increment-mediator" namespace="fr.imag.adele.cilia.workbench.spec">
		<ports>
			<in-port name="in"/>
			<out-port name="out"/>
		</ports>
		<properties>
			<property name="category"/>
		</properties>
		<processor>
			<parameters>
				<parameter name="increment"/>
			</parameters>
		</processor>
	</mediator-specification>

	<mediator-specification id="sum-mediator" namespace="fr.imag.adele.cilia.workbench.spec">
		<ports>
			<in-port name="in"/>
			<out-port name="out"/>
		</ports>
		<properties>
			<property name="category"/>
		</properties>
	</mediator-specification>

