<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<cilia>
	<chain id="test" namespace="">
		<adapters>
			<adapter-instance id="generator" namespace="fr.liglab.adele.cilia"
				type="random-generator" />
			<adapter-instance id="console" namespace="fr.liglab.adele.cilia"
				type="console-adapter" />
		</adapters>
		<mediators>
			<mediator-instance id="groovy" namespace="fr.liglab.adele.cilia"
				type="groovy-mediator" />
		</mediators>
		<bindings>
			<binding from="generator:out" to="groovy:in" />
			<binding from="groovy:out" to="console:unique" />
		</bindings>
	</chain>
</cilia>
